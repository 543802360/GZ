<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getCompanyByName</servlet-name>
    <servlet-class>edu.whu.servlet.getCompanyByName</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getCompanyNames</servlet-name>
    <servlet-class>edu.whu.servlet.getCompanyNames</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getKeyCompanyInfo</servlet-name>
    <servlet-class>edu.whu.servlet.getKeyCompanyInfo</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getExportImportInfo</servlet-name>
    <servlet-class>edu.whu.servlet.getExportImportInfo</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>getCompanyByName</servlet-name>
    <url-pattern>/servlet/getCompanyByName</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getCompanyNames</servlet-name>
    <url-pattern>/servlet/getCompanyNames</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getKeyCompanyInfo</servlet-name>
    <url-pattern>/servlet/getKeyCompanyInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getExportImportInfo</servlet-name>
    <url-pattern>/servlet/getExportImportInfo</url-pattern>
  </servlet-mapping>

</web-app>